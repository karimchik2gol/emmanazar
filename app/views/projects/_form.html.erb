<div class="row">
  <div class="col-sm-11">
    <%= form_for(project) do |f| %>
      <% if project.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(project.errors.count, "error") %> prohibited this project from being saved:</h2>

          <ul>
          <% project.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      <% end %>

      <div class="field">
        <%= f.label :name %><br/>
        <%= f.text_field :name, :class => "form-control inputmask" %>
      </div>

      <div class="field">
        <%= f.label :zabydovnuk_id %><br/>
        <%= f.collection_select :zabydovnuk_id, Zabydovnuk.all, :id, :name, {}, { :class => "form-control select2" } %>
      </div>

      <div class="form-group">
        <button type="submit" class="btn btn-success waves-effect waves-light btn-md">Submit</button>
      </div>
    <% end %>
  </div>
</div>
